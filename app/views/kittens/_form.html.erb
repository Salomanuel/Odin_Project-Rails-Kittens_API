<%= form_for @kitten do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	
	<%= f.label :age %>
	<%= f.select(:age,			options_for_select(1..30)) %>

	<%= f.label :cuteness %>
	<%= f.select(:cuteness, options_for_select(0..10)) %>

	<%= f.label :softness %>
	<%= f.select(:softness, options_for_select(0..10)) %>

	<%= f.submit %>
<% end %>